{
    "name": "dendron-snippet-maker",
    "displayName": "dendron-snippet-maker",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "dendron.gotoGlobalSnippet",
                "title": "Goto Global Snippet"
            },
            {
                "command": "dendron.createGlobalSnippet",
                "title": "Create Global Snippet"
            }
        ],
        "configuration": {
            "properties": {
                "dendron.dailyJournalDomain": {
                    "type": "string",
                    "default": "daily",
                    "description": "domain where daily journals are kept"
                }
            }
        },
        "keybindings": []
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/comment-json": "^1.1.1",
        "@types/fs-extra": "^9.0.1",
        "@types/glob": "^7.1.1",
        "@types/lodash": "^4.14.161",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "@types/vscode": "^1.48.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "comment-json": "^4.0.4",
        "fs-extra": "^9.0.1",
        "lodash": "^4.17.20"
    }
}
